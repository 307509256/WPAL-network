train_net: "models/VGG_CNN_S_single_level/train_net.prototxt"

base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 350000
display: 100
average_loss: 100
momentum: 0.9
weight_decay: 0.00005

# Standard caffe solver snapshotting is replaced by our own snapshot function
snapshot: 0

# We still use the snapshot prefix, though
snapshot_prefix: "wma_s_sl"

iter_size: 2
